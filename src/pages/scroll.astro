---
import Layout from "@/components/Layout.astro"
import FreeScrollCarousel from "@/components/ui/FreeScrollCarousel.tsx"
---

<Layout>
  {
    Array.from([1, 2, 3], (_, i) => (
      <div key={i} class="">
        <img src={`/images/${i + 1}.jpg`} alt="" />
      </div>
    ))
  }

  <FreeScrollCarousel images={[4, 5, 6]} client:load />

  {
    Array.from([7, 8, 9], (i) => (
      <div key={i} class="">
        <img src={`/images/${i}.jpg`} alt="" />
      </div>
    ))
  }
</Layout>
